[server]
http_port = {{ grafana_port }}

[security]
admin_password = {{ grafana_admin_password }}

[dataproxy]
http_timeout = 30s

[dashboard]
enabled = true
provisioning = {{ grafana_provisioning_dir }}

[smtp]
enabled = true
host = {{ email_host }}
user = {{ email }}
password = {{ email_password }}
skip_verify = true
from_address = {{ email }}
from_name = Grafana Alerts
ehlo_identity = grafana.local
starttls_policy = IfAvailable
